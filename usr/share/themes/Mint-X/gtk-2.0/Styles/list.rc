style "icon-view"
{
	engine "pixmap"
	{	
		image 
      		{
			function			= FLAT_BOX
			recolorable			= TRUE
			file				= "Treeview/iconview-selected.png" 
			border				= { 8, 8, 8, 8}
			stretch				= TRUE
		}
	}
}
