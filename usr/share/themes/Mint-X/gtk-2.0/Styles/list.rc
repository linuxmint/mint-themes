style "treeview" = "default"
{
	base[SELECTED]					= shade (0.8, @selected_bg_color)
	base[ACTIVE]					= shade (0.8, @selected_bg_color)

	xthickness					= 1
	ythickness					= 1

}

style "treeview-header" 				= "button"
{
	xthickness					= 2
	ythickness					= 1				

	bg[NORMAL]      				= shade (1.04, @bg_color)
	#bg[PRELIGHT]    				= shade (1.11, @bg_color)
	bg[ACTIVE]      				= shade (0.92, @bg_color)
	bg[INSENSITIVE] 				= shade (1.04, @bg_color)

	engine "murrine"
	{
		reliefstyle	 			= 4
		lightborderstyle 			= 1
	}
}

style "pixmap-treeview" 
{
	GtkTreeView::row-ending-details 		= 1
	GtkTreeView::horizontal-separator 		= 2
	GtkTreeView::vertical-separator 		= 2
	GtkTreeView::allow-rules 			= 0
	GtkTreeView::rules-hint  			= 0
	GtkTreeView::expander-size 			= 14

	engine "pixmap"									
	{
	#ROWS	
		image
		{
			function			= FLAT_BOX
			recolorable			= TRUE
			state				= NORMAL
			file				= "Null/null.png"
			stretch				= TRUE
		}
		image
		{
			function			= FLAT_BOX
			recolorable			= TRUE
			state				= SELECTED
			file				= "Treeview/row-selected.png"
			border				= { 0, 2, 2, 2}
			stretch				= TRUE
		}
		image
		{
			function			= FOCUS
			file				= "Null/null.png"
		}
		#EXPANDERS
		image
 		{
			function			= EXPANDER
			state				= NORMAL
			expander_style			= COLLAPSED
			file				= "Treeview/expander-collapsed.png"
		}
		image
 		{
			function			= EXPANDER
			state				= PRELIGHT
			expander_style			= COLLAPSED
			file				= "Treeview/expander-collapsed-prelight.png"
		}
		image
 		{
			function			= EXPANDER
			state				= ACTIVE
			expander_style			= COLLAPSED
			file				= "Treeview/expander-collapsed.png"
		}
		#semi collapsed
      		image
 		{
			function			= EXPANDER
			state				= NORMAL
			expander_style			= SEMI_COLLAPSED
			file				= "Treeview/expander-semi-collapsed.png"
		}
		image
 		{
			function			= EXPANDER
			state				= PRELIGHT
			expander_style			= SEMI_COLLAPSED
			file				= "Treeview/expander-semi-collapsed-prelight.png"
		}
		image
 		{
			function			= EXPANDER
			state				= ACTIVE
			expander_style			= SEMI_COLLAPSED
			file				= "Treeview/expander-semi-collapsed.png"
		}
		#semi expanded
	      	image
 		{
			function			= EXPANDER
			state				= NORMAL
			expander_style			= SEMI_EXPANDED
			file				= "Treeview/expander-semi-expanded.png"
		}
		image
 		{
			function			= EXPANDER
			state				= PRELIGHT
			expander_style			= SEMI_EXPANDED
			file				= "Treeview/expander-semi-expanded-prelight.png"
		}
		image
 		{
			function			= EXPANDER
			state				= ACTIVE
			expander_style			= SEMI_EXPANDED
			file				= "Treeview/expander-semi-expanded.png"
		}
		#expanded
      		image
 		{
			function			= EXPANDER
			state				= NORMAL
			expander_style			= EXPANDED
			file				= "Treeview/expander-expanded.png"
			detail				= "treeview"
		}
		image
 		{
			function			= EXPANDER
			state				= PRELIGHT
			expander_style			= EXPANDED
			file				= "Treeview/expander-expanded-prelight.png"
			detail				= "treeview"
		}
		image
 		{
			function			= EXPANDER
			state				= ACTIVE
			expander_style			= EXPANDED
			file				= "Treeview/expander-expanded.png"
			detail				= "treeview"
		}
		image
 		{
			function			= HLINE
			state				= NORMAL
			file				= "Treeview/hline.png"
			border				= { 0, 0, 0, 0}
		}
		image
 		{
			function			= HLINE
			state				= PRELIGHT
			file				= "Treeview/hline.png"
			border				= { 0, 0, 0, 0}
		}
		image
 		{
			function			= HLINE
			state				= ACTIVE
			file				= "Treeview/hline.png"
			border				= { 0, 0, 0, 0}
		}
		image
	 	{
			function			= HLINE
			state				= SELECTED
			file				= "Treeview/hline.png"
			border				= { 0, 0, 0, 0}
		}
		image
 		{
			function			= HLINE
			state				= INSENSITIVE
			file				= "Treeview/hline.png"
			border				= { 0, 0, 0, 0}
		}
		#PROGRESSBAR INSIDE THE TREEVIEW
		image
 		{
			function			= BOX
			state				= NORMAL
			file				= "Treeview/Progressbar/trough.png"
			border				= { 2, 2, 2, 2}
			shadow				= IN
			#detail				= "trough"
		}
		image
 		{
			function			= BOX
			state				= SELECTED
			file				= "Treeview/Progressbar/bar.png"
			stretch				= TRUE
			border				= { 2, 2, 2, 2}
			orientation			= HORIZONTAL
			shadow				= OUT
			detail				= "bar"
		}
		#RADIO/CHECK IN TREEVIEW
		#checkbuttons
		image
		{
			function			= CHECK
			state				= NORMAL
			shadow				= OUT
			overlay_file			= "Radiocheck/check-shadow-out.png"
			overlay_stretch			= FALSE
		}
