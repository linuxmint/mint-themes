			state				= INSENSITIVE
			shadow				= IN
			overlay_file			= "Radiocheck/check-i-shadow-in.png"
			overlay_stretch			= FALSE
		}
		#radiobuttons
		image
		{
			function			= OPTION
			state				= NORMAL
			shadow				= OUT
			overlay_file			= "Radiocheck/radio-shadow-out.png"
			overlay_stretch			= FALSE
		}
		image
		{
			function			= OPTION
			state				= PRELIGHT
			shadow				= OUT
			overlay_file			= "Radiocheck/radio-p-shadow-out.png"
