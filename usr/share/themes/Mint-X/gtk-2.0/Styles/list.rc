		{
			function			= CHECK
			state				= PRELIGHT
			shadow				= OUT
			overlay_file			= "Radiocheck/check-p-shadow-out.png"
			overlay_stretch			= FALSE
		}
		image
		{
			function			= CHECK
			state				= ACTIVE
			shadow				= OUT
			overlay_file			= "Radiocheck/check-shadow-out.png"
			overlay_stretch			= FALSE
